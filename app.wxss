.bline, .rline {
    position: relative;
}
.inline{
  display:inline-block;
}
page {
    background: #f4f4f4;
    color: #444;
    padding-bottom:100rpx;
}

.page-scroll {
    position: absolute;
    left: 0;
    right: 0;
    top: 90rpx;
    bottom: 0;
}

.page-scroll .empty {
    padding-top: 20%;
}

.mask {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    background: rgba(0, 0, 0, 0.5);
    z-index: 90;
}

.bline:after {
    content: '';
    position: absolute;
    left: -50%;
    right: -50%;
    bottom: 0;
    border-bottom: 1px solid #eee;
    transform: scale(0.5);
}

.rline:before {
    content: '';
    position: absolute;
    top: -50%;
    right: 0;
    bottom: -50%;
    border-right: 1px solid #eee;
    transform: scale(0.5);
}

.blines:before {
    content: '';
    position: absolute;
    left: -50%;
    right: -50%;
    bottom: 0;
    border-bottom: 1px solid #eee;
    transform: scale(0.5);
}

.f-c-c {
    display: flex;
    align-items: center;
    justify-content: center;
}

.f-c-c-co {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
}

.f-c {
    display: flex;
    align-items: center;
}

.f-c-sb {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.hidden {
    display: none;
}

@font-face {
    font-family: 'lianyou';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
    /* use !important to prevent issues with browser extensions that change fonts */
    font-family: 'lianyou' !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    /* Better Font Rendering =========== */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-empty:before {
    content: "\e902";
}

.icon-grid:before {
    content: "\e90a";
}

.icon-list:before {
    content: "\e90b";
}

.icon-report:before {
    content: "\e908";
}

.icon-search:before {
    content: "\e909";
}

.icon-base:before {
    content: "\e900";
}

.icon-del:before {
    content: "\e901";
}

.icon-lock:before {
    content: "\e903";
}

.icon-logo:before {
    content: "\e904";
}

.icon-oks:before {
    content: "\e905";
}

.icon-phone:before {
    content: "\e906";
}

.icon-user:before {
    content: "\e907";
}

.empty {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    font-size: 24rpx;
    color: #999;
}

.empty .icos {
    margin-top: -20%;
}

.empty .ico {
    font-size: 70rpx;
    position: relative;
    animation: sliderInUp 0.5s ease both;
}

.empty text {
    font-size: 26rpx;
    margin-top: 20rpx;
    display: block;
    position: relative;
    text-align: center;
    animation: sliderInUp 0.5s 0.2s ease both;
}

.button {
    background: #ff6363;
    color: #fff;
    border-radius: 4rpx;
    display: block;
    margin-left: auto;
    margin-right: auto;
    padding-left: 14px;
    padding-right: 14px;
    box-sizing: border-box;
    font-size: 32rpx;
    text-align: center;
    text-decoration: none;
    line-height: 2.55555556;
    transition: 0.3s;
    -webkit-tap-highlight-color: transparent;
}

.button .ico {
    margin-right: 18rpx;
}

.button-gray {
    background: gray;
    color: #fff;
}

.button-min {
    line-height: normal;
    margin: 0;
}

.button-touch {
    background: #df4b4b;
}

.button-red {
    background: #f56e50;
}

.button-red-touch {
    background: #da5234;
}

.button-shadow {
    box-shadow: 0 10px 40px rgba(80, 80, 80, 0.2);
}

.button::after {
    border: none;
}
.button.disable{
    opacity:.6;
}
@keyframes sliderInUp {
    0% {
        transform: translateY(100%);
    }

    100% {
        transform: translateY(0);
    }
}

.more {
    text-align: center;
    padding: 40rpx 0;
    color: #999;
    font-size: 24rpx;
}

.l-bottom {
    text-align: center;
    font-size: 28rpx;
    color: #999;
    opacity: 0.8;
    display: flex;
    align-items: center;
    justify-content: center;
    height: 150rpx;
}

.list>view {
    padding: 0 40rpx;
}

.list-arr {
    position: relative;
    display: flex;
    transition: 0.4s;
}

.list-arr-touch {
    background: #eee;
}

.list-arr::after {
    content: '';
    display: block;
    width: 13rpx;
    height: 13rpx;
    border: 2px solid #eee;
    border-left: none;
    border-bottom: none;
    transform: rotate(45deg);
    margin-left: 10rpx;
}

.touch {
    opacity: 0.7;
}

.add {
    border: 1px dashed #ff6363;
    color: #ff6363;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 22rpx 0;
    font-size: 28rpx;
    margin: 0 40rpx;
    border-radius: 6rpx;
    margin-bottom: 40rpx;
}

.add .ico {
    margin-right: 12rpx;
    font-size: 22rpx;
    color: #ff6363;
}

.form-bar {
    margin: 50rpx 40rpx 180rpx;
}

/* switch style */

/*swtich整体大小及背景色*/

.wx-switch-input {
    width: 80rpx !important;
    height: 40rpx !important;
    background: #fff !important;
    border: none !important;
}

/*白色样式（false的样式）*/

.wx-switch-input::before {
    width: 80rpx !important;
    height: 40rpx !important;
    background: #eee !important;
}

/*绿色样式（true的样式）*/

.wx-switch-input.wx-switch-input-checked {
    background: #ff6363 !important;
}

.wx-switch-input::after {
    width: 40rpx !important;
    height: 40rpx !important;
}

/* search start// */

.search {
    height: 90rpx;
    display: flex;
    align-items: center;
    padding: 0 20rpx;
    background: #fff;
    /* box-shadow: 0 12px 40px rgba(90, 90, 90, 0.1); */
}

.search .ico {
    font-size: 34rpx;
    color: #999;
    line-height: normal;
    margin-right: 15rpx;
}

.search-del {
    font-size: 32rpx;
    background: #fff;
    padding: 20rpx 20rpx;
    border-radius: 3rpx;
    color: #999;
}

.search-del-touch {
    color: #ccc;
}

.search-mask {
    background: rgba(0, 0, 0, 0.3);
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    top: 188rpx;
}

.search input {
    font-size: 28rpx;
    flex-grow: 2;
    color: #444;
}

/* //end search */

/* ??tab */

.tab {
    display: flex;
    background: #fff;
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    z-index: 10;
}

.tab>view {
    width: 25%;
    border-bottom: 3px solid transparent;
    text-align: center;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 29rpx;
    color: #666;
    font-weight: 800;
    transition: 0.4s;
    box-sizing:border-box;
}

.tab>view.active {
    border-bottom: 3px solid #ff6363;
    color: #ff6363;
}

