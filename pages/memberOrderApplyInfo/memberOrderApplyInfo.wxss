.conts{
    background:#fff;
    font-size:28rpx;
    padding:40rpx;
    margin:0;
}
.cont-title{
    font-weight:800;
    padding:30rpx 0;
}

.conts-phone{
    font-size:30rpx;
}

.conts-item{
    padding:26rpx 0;
    display:flex;
    color:#666;
}
.conts>.bline::first-child{
    margin-top:50rpx;
}
.conts-name{
    font-weight:800;
    font-size:36rpx;
    margin-bottom:20rpx;
}
.conts .conts-item:last-child::after{
    border:none;
}

