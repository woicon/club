.a-ticket{
    display: flex;
}
.a-head {
    display:flex;
    align-items:center;
    background:#fff;
    padding:20rpx 40rpx;
    margin-bottom:10rpx;
}
.a-head image{
    width:250rpx;
    height:146rpx;
    margin:0;
    padding:0;
    vertical-align:middle;
    border-radius:8rpx;
}
.a-title{
    font-weight:800;
    font-size:30rpx;
}
.dates{
    padding:10rpx 20rpx;
    background:#fff;
}
.dates .ico{
    font-size:22rpx;
}
.dates>view{
    display: flex;
    align-items:center;
    padding:10rpx 0;
    font-size:22rpx;
    color:#666;
}
.dates .a-title{
    font-size:30rpx;
    color:#FF6363;
}
.dates .ico{
    margin-right:10rpx;
}
.a-ticket>div{
    
}
.a-block{
    background:#fff;
    padding:30rpx 40rpx;
}
.csblock{
    display:flex;
    justify-content:space-between;
    flex-wrap:wrap;
}
.items{
    width:46%;
    
    font-size:26rpx;
    background:#eee;
    color:#333;
    padding:20rpx;
    text-align:center;
}
.items>view{
    color:#FF6363;
    font-weight:800;
}
.cstitle{
    font-weight:800;
    font-size:28rpx;
    margin-bottom:20rpx;
}
