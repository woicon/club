/* pages/memberOderDetail/memberOderDetail.wxss */
.a-head {
    display:flex;
    align-items:center;
    background:#fff;
    padding:20rpx 40rpx;
    margin-bottom:10rpx;
}
.a-head image{
    width:250rpx;
    height:146rpx;
    margin:0;
    padding:0;
    vertical-align:middle;
    border-radius:8rpx;
}
.a-title{
    font-weight:800;
    font-size:30rpx;
}
.dates{
    padding:10rpx 20rpx;
    background:#fff;
}
.dates .ico{
    font-size:22rpx;
}
.dates>view{
    display: flex;
    align-items:center;
    padding:10rpx 0;
    font-size:22rpx;
    color:#666;
}
.dates .a-title{
    font-size:30rpx;
    color:#FF6363;
}
.dates .ico{
    margin-right:10rpx;
}